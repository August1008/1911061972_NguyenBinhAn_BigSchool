@model IEnumerable<_1911061972_NguyenBinhAn_BigSchool.Models.Course>
@{
    ViewBag.Title = "Home Page";
}
<head>
    <link rel="stylesheet" href="~/Content/Site.css" />
</head>

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
    
</div>

       <ul class="course">
           @foreach(var course in Model)
            {
           <li>
               <div class="date">
                   <div class="month">@course.datetime.ToString("MM")</div>
                   <div class="day">@course.datetime.ToString("dd")</div>
               </div>

               <div class="details">
                   <div class="lecturer">@course.Lecturer.name</div>
                   <div class="category">@course.Category.name</div>
               </div>

           </li>
            }
       </ul>

       
    